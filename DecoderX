import os, random,sys,time
def print_slowly(text, delay=0.1):
   for char in text:
       print(char, end='', flush=True)
       time.sleep(delay)
   print()
from cfonts import render
P = '\x1b[1;97m'
H = '\x1b[1;92m'
K = '\x1b[1;93m'
k = '\x1b[93m'
m = '\x1b[1;91m'
k = '\x1b[93m'
h = '\x1b[1;92m'
u = '\x1b[95m'
kk = '\x1b[33m'
b = '\x1b[1;96m'
N = '\x1b[0m'
asu = random.choice([m, k, h, u, b])
menu = render('JOKER', colors=['green', 'white'], align='center')
print(menu)
print('')
