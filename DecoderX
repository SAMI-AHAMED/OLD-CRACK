print('— — — — — — — — — — — — — — — — — — — — — — — — — — — — — —')
print(f'                  {P}[{H} 𝗩𝗘𝗥𝗦𝗜𝗢𝗡 - 𝗔𝗹𝗹 𝗶𝗻 𝗢𝗻𝗲{H}{P}]')
print('— — — — — — — — — — — — — — — — — — — — — — — — — — — — — —')

print()
